    pod 'Alamofire', '~> 4.0'
    pod 'AlamofireObjectMapper', '~> 4.0'
    pod 'SwiftyJSON'
    pod 'ObjectMapper+Realm'
    pod 'Kingfisher', '~> 3.0'
end

target 'WatchMeUITests' do
    pod 'Alamofire', '~> 4.0'
    pod 'AlamofireObjectMapper', '~> 4.0'
    pod 'SwiftyJSON'
    pod 'ObjectMapper+Realm'
